{
    "name": "rhincodon/laravel-letsads",
    "description": "Laravel LetsAds SMS Service Provider",
    "keywords": [
        "laravel",
        "letsads",
        "sms"
    ],
    "homepage": "https://github.com/rhincodon/laravel-letsads",
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Isaychenko",
            "email": "rhinodontypicus@gmail.com",
            "homepage": "http://laravelinfo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "illuminate/support": "~5.1.0|~5.2.0|~5.3.0",
        "guzzlehttp/guzzle": "~6.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "orchestra/testbench": "^3.3",
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Rhinodontypicus\\LetsAds\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rhinodontypicus\\LetsAds\\Test\\": "tests"
        }
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
